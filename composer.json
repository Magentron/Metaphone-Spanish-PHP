{
    "name": "magentron/metaphone-spanish",
    "description": "PHP implementation of the Spanish metaphone algorithm",
    "type": "library",
	"keywords": ["PHP","metaphone","spanish"],
	"homepage": "https://github.com/Magentron/Metaphone-Spanish-PHP",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Jeroen Derks",
            "email": "jeroen@derks.it",
			"homepage": "https://www.derks.it/",
			"role": "Developer"
        }
    ],
    "minimum-stability": "beta",
    "autoload": {
		"psr-4": {
			"Magentron\\MetaphoneSpanish\\": "src/",
			"Magentron\\MetaphoneSpanish\\Tests\\": "tests/"
		}
	},
	"bin": [
		"bin/metaphone-es"
	],
    "repositories": [
        {
          "type": "package",
          "package": {
            "name": "amsqr/spanish-metaphone",
            "version": "dev-master",
            "source": {
              "url": "https://github.com/amsqr/Spanish-Metaphone",
              "type": "git",
              "reference": "origin/master"
            }
          }
        }
    ],
    "require-dev": {
		"php": "^7.3",
        "amsqr/spanish-metaphone": "dev-master",
        "phpstan/phpstan": "0.12.x-dev",
        "friendsofphp/php-cs-fixer": "9999999-dev",
        "phpmd/phpmd": "9999999-dev",
        "squizlabs/php_codesniffer": "4.0.x-dev",
        "sebastian/phpcpd": "6.0.x-dev",
        "phploc/phploc": "7.0.x-dev",
        "phpunit/phpunit": "^9.5",
        "theseer/phpdox": "dev-master",
        "clean/phpdoc-md": "^0.19.1"
    }
}
